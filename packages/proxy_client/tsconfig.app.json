{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "rootDir": "../..",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "types/*": ["./src/types/*"],
      "utils/*": ["./src/utils/*"],
      "commonTypes/*":["../types/*"]
    }
  }
}
